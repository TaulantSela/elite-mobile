{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/(css|js|img|vendor|styles|bootstrap-3\\.3\\.7-dist|screenshot)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
